    {
      "description": "child object is missing",
      "test-value": {
        "Prop1": "a",
        "Prop2": "b",
        "Prop3": "c"
      },
      "expected-failures": [
        "Core || / || Required property doesn't exist: nested"
      ]
    }
  ]
